{
  "name": "studio",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "deploy-graphql": "sanity graphql deploy",
    "extract": "sanity schema extract --enforce-required-fields",
    "check-types": "tsc --noEmit",
    "check": "npm run lint && npm run format && npm run check-types",
    "check:fix": "npm run lint:fix && npm run format:fix",
    "type": "sanity schema extract --enforce-required-fields && sanity typegen generate"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/assist": "^5.0.0",
    "@sanity/color-input": "^4.0.6",
    "@sanity/icons": "^3.7.4",
    "@sanity/orderable-document-list": "^1.4.0",
    "@sanity/ui": "^3.0.7",
    "@sanity/vision": "^4.4.1",
    "lucide-react": "^0.539.0",
    "react": "^19.1",
    "react-dom": "^19.1",
    "sanity": "^4.4.1",
    "sanity-plugin-asset-source-unsplash": "^4.0.1",
    "sanity-plugin-icon-picker": "^4.0.0",
    "sanity-plugin-media": "^4.0.0",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.1",
    "eslint": "^9.28",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.5",
    "typescript-eslint": "^8.39.1"
  }
}
